algoritmo "ex. 1"
var
   nome: caractere
   idade: inteiro
   nota: real
inicio
      escreva("Digite seu nome: ")
      leia(nome)
      escreva("Digite sua idade: ")
      leia(idade)
      escreva("Digite sua nota: ")
      leia(nota)
      escreval("Nome: ", nome)
      escreval("Idade:", idade)
      escreval("Nota:", nota)
      
fimalgoritmo
